{% comment %} add bg image {% endcomment %}

{% if request.page_type == 'product'
  and template.name == 'product'
  and template.suffix == 'metafield'
  and section.settings.bg_image != blank
%}
  {% style %}
    body.template-product-metafield {
      background-image: url({{ section.settings.bg_image | image_url: width: 2400 }});
      background-size: cover;
      background-repeat: no-repeat;
      background-position: center;
      min-height: 100vh;
    }
  {% endstyle %}
{% endif %}


{% comment %} bg image end {% endcomment %}





{% for block in section.blocks %}
  {% if block.type == 'custom-fbt' %}
    {% if block.settings.products.count > 0 %}
    {{ 'd8-custom-fbt.css' | asset_url | stylesheet_tag }}
    {% endif %}
  {% endif %}
{% endfor %}


