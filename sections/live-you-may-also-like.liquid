</style>
 	
  	<div class="group-parent" id="section--{{ section.id }}">
    		<div class="you-may-also-like-parent">
      			<div class="you-may-also">{{ section.settings.main-heading }}</div>
      			<div class="frame-parent">
                  {% for block in section.blocks %}
        				<a href="{{ block.settings.my-product.url }}" class="image-75-parent {% if forloop.index > 2 %} extra-products {% endif %}" >
          				<div>	
                          <img class="image-75-icon" alt="" src="{{ block.settings.my-product.featured_image | img_url:"1440x" }}">
          				 </div>
          					<div class="brightlink-86-4k">{{ block.settings.my-product.title | truncatewords:4 }}</div>
          					<div class="ellipse-parent">
            						<div class="frame-child">
            						</div>
            						<div class="in-stock">{% if block.settings.my-product.available %} In stock {% else %} Out of Stock {% endif %}</div>
          					</div>
          					<div class="parent">
            						<div class="div">{{ block.settings.my-product.price | money }}</div>
            						<div class="div1">{{ block.settings.my-product.compare_at_price | money }}</div>
          					</div>
        				</a>
                    {% endfor %}
      			</div>
    		</div>
  	</div>





{% schema %}
  {
    "name": "live you may also like",
    "settings": [
     {
       "type":"text",
       "id":"main-heading",
       "label":"Heading"
     }           
    ],
    "blocks":[
     {
      "type": "product",
      "name": "Product",
      "settings": [
      {
       "type":"product",
       "id":"my-product",
       "label":"Select a product"
     }
      ]
      }
    ],
    "presets":[
      {
        "name":"live you may also like"
      }
    ]
  }
{% endschema %}

