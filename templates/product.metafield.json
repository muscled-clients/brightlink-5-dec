        "7114b4b1-a69e-40bb-b407-6775e44d2375": {
          "type": "question",
          "settings": {
            "title": "What payment methods do you accept?",
            "answer": "<p>We accept most major credit card directly through our website. We also accept PayPal, Bank Transfer, and Check. For Bank Transfer or Check you will need to email support@brightlinkav.com to request instructions. For all payment methods we require payment in full prior to shipping any goods.</p>"
          }
        },
        "7caea11b-79c7-438d-9a96-cc48bf39384e": {
          "type": "question",
          "settings": {
            "title": "Do you any Credit or Terms?",
            "answer": "<p>No we do not offer any Credit or terms. If you need credit or terms you can contact our distributor https://microage.com/ - 480-366-2072 - jp@microage.com</p>"
          }
        },
        "eab21537-4412-4e29-94dd-9d50b7cf8b0b": {
